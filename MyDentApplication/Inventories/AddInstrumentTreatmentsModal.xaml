<Window
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	x:Class="MyDentApplication.AddInstrumentTreatmentsModal"
	x:Name="Window"
	Title="Agregar tratamiento"
	Width="410" Height="122" ResizeMode="CanMinimize" Icon="/MyDentApplication;component/app_icon.ico" WindowStartupLocation="CenterScreen">

	<Grid x:Name="LayoutRoot">
		<ComboBox x:Name="cbTratmentName" Margin="95.553,9.96,20,0" Height="26" VerticalAlignment="Top" FontWeight="Normal" TabIndex="2"/>
		<Label x:Name="lblTratmentNameCaption" Content="Tratamiento:" Margin="12.106,10,0,0" FontWeight="Normal" HorizontalAlignment="Left" Width="78.447" Height="25" VerticalAlignment="Top"/>
		<Button x:Name="btnAccept" Content="Aceptar" HorizontalAlignment="Left" Margin="130.5,0,0,16" Width="70" Cursor="Hand" Click="btnAccept_Click" TabIndex="3" Height="25" VerticalAlignment="Bottom"/>
		<Button x:Name="btnCancel" Content="Cancelar" HorizontalAlignment="Left" Margin="205.5,0,0,16" Width="70" Click="btnCancel_Click" Cursor="Hand" TabIndex="4" Height="25" VerticalAlignment="Bottom"/>
	</Grid>
</Window>