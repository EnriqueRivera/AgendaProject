<Window
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	x:Class="MyDentApplication.RequestEmailModal"
	x:Name="Window"
	Title="Email"
	Width="540.701" Height="169" Icon="/MyDentApplication;component/Configuration/app_icon.ico" ResizeMode="CanMinimize" WindowStartupLocation="CenterScreen">
	<Grid x:Name="LayoutRoot" Margin="0,0,-3,-4.97">
		<Button x:Name="btnAccept" Content="Aceptar" HorizontalAlignment="Left" Height="24.97" Margin="163.845,0,0,15" VerticalAlignment="Bottom" Width="90" Cursor="Hand" Click="btnAccept_Click" TabIndex="3"/>
		<Button x:Name="btnCancel" Content="Cancelar" HorizontalAlignment="Left" Height="24.97" Margin="258.845,0,0,15" VerticalAlignment="Bottom" Width="90" Cursor="Hand" Click="btnCancel_Click" TabIndex="4"/>
		<Label x:Name="lbPurchaseDate" Content="Correo:" HorizontalAlignment="Left" Height="27" Margin="109.311,54.384,0,0" VerticalAlignment="Top" Width="52.534"/>
		<Label x:Name="lbPurchaseDate_Copy" Content="El paciente no cuenta con un correo electrónico, introduzca alguno para poder enviar el folio." Height="27" Margin="10,10,10,0" VerticalAlignment="Top"/>
		<TextBox x:Name="txtEmail" HorizontalAlignment="Left" Height="27" Margin="166.845,54.384,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="228" TabIndex="1" BorderBrush="Black"/>
	</Grid>
</Window>